@model AddFamilyMemberForm
@{
    ViewData["Title"] = "Add Family Member";
}

<h1>Add Family Member</h1>

<form class="col-xl-4">
    <div class="form-group">
        <label asp-for="SearchEmail"></label>
        <input class="form-control" asp-for="SearchEmail" placeholder="address@@email.com"/>
    </div>
    <div class="form-group">
        <input class="btn btn-primary" type="submit" />
    </div>
</form>

<hr />

<div class="search-results">

</div>

<script src="~/js/Family/AddMember.js"></script>